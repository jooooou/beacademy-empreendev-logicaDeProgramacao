algoritmo "exercicio4"
// saldo inicial, deposito, saldo inicial, saldo atual
var
saldoInicial, deposito: REAL
inicio
escreval("Digite seu saldo: ")
leia(saldoInicial)
escreval("Digite o valor do depósito: ")
leia(deposito)
escreval("Seu saldo inicial era: " , saldoInicial)
escreval("Seu saldo atual é: " , (saldoInicial) + (deposito) )
fimalgoritmo
